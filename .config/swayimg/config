[keys.viewer]
h = prev_file
j = next_file
k = prev_file
l = next_file
Shift+d = exec trash -v '%'; skip_file
Space = mode gallery

[keys.gallery]
h = step_left
j = step_down
k = step_up
l = step_right
Shift+d = exec trash -v '%'; skip_file
Space = mode viewer

[list]
# Add files from the same directory as the first file (yes/no)
all = yes
# Read directories recursively (yes/no)
recursive = no
